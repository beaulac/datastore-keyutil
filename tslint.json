{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      "single"
    ],
    "no-consecutive-blank-lines": [
      false
    ],
    "interface-name": [
      "never-prefix"
    ],
    "member-ordering": false,
    "trailing-comma": false
  },
  "rulesDirectory": []
}
